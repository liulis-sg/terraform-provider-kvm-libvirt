  <domain type="kvm">
      <name>windows-1</name>
      <metadata><libosinfo:libosinfo xmlns:libosinfo="http://libosinfo.org/xmlns/libvirt/domain/1.0"><libosinfo:os id="http://microsoft.com/win/10"/></libosinfo:libosinfo></metadata>
      <memory unit="MiB">4192</memory>
      <memoryBacking>
          <hugepages>
              <page size="2048" unit="KiB"></page>
          </hugepages>
          <nosharepages></nosharepages>
          <source type="memfd"></source>
          <access mode="shared"></access>
          <allocation mode="immediate"></allocation>
      </memoryBacking>
      <vcpu>6</vcpu>
      <os>
          <type arch="x86_64" machine="pc-q35-6.0">hvm</type>
      </os>
      <features>
          <pae></pae>
          <acpi></acpi>
          <apic></apic>
      </features>
      <cpu mode="host-passthrough"></cpu>
      <devices>
          <emulator>/usr/bin/qemu-system-x86_64</emulator>
          <disk type="file" device="disk">
              <driver name="qemu" type="qcow2"></driver>
              <source file="/home/user/Downloads/windows.qcow2"></source>
              <target dev="vda" bus="virtio"></target>
          </disk>
          <interface type="network">
              <mac address="52:54:00:ea:eb:09"></mac>
              <source network="vm-default"></source>
              <model type="virtio"></model>
          </interface>
          <graphics type="vnc" autoport="yes">
              <listen type="address" address="0.0.0.0"></listen>
          </graphics>
          <video>
              <model type="qxl"></model>
          </video>
          <hostdev mode="subsystem" type="pci">
              <driver name="vfio"></driver>
              <source>
                  <address slot="0x02" function="0x2"></address>
              </source>
          </hostdev>
          <rng model="virtio">
              <backend model="random">/dev/urandom</backend>
          </rng>
      </devices>
  </domain>